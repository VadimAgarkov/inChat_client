{ 
  "parserOptions": {
    "sourceType": "module",
    "parser": "@babel/eslint-parser",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "tsx": true
    }
  },
  "rules": {
    "no-confusing-arrow": "off",
    "no-console": "error",
    "quotes": ["error", "single"],
    "no-multiple-empty-lines": ["error", {"max": 1}],
    "no-var": "error",
    "prefer-const": "error"
  }
}
